
window.onload = function(){
document.getElementById("btitulo").innerHTML = perfil.nombre;
document.getElementById("ccolor").innerHTML = config.color;
document.getElementById("jcolor").innerHTML = perfil.color;
document.getElementById("clibro").innerHTML = config.libro;
document.getElementById("jlibro").innerHTML = perfil.libro;
document.getElementById("cmusica").innerHTML = config.musica;
document.getElementById("jmusica").innerHTML = perfil.musica;
document.getElementById("cjuego").innerHTML = config.video_juego;
document.getElementById("jjuego").innerHTML = perfil.video_juego;
document.getElementById("clenguaje").innerHTML = config.lenguajes;
document.getElementById("jlenguajes").innerHTML = perfil.lenguajes;
document.getElementById("cmail").innerHTML = config.email;
document.getElementById("jmail").innerHTML = perfil.email;
};
